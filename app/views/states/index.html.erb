<h3> <%= link_to 'States', 'states#index' %> </h3>
<h3> <%= link_to 'Cities', 'cities#index' %> </h3>

<h1> States </h1>

<% @states.each_with_index do |state, index| %>
<div id="state-<%= index %>">
    <p><b>  <%= state.name %></b></p>
    <p> Created at: <%= state.created_at %> </p>
    </div>
<% end %>

