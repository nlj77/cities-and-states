<h3> <%= link_to 'States', '/states' %> </h3>
<h3> <%= link_to 'Cities', '/cities' %> </h3>

<h1> Cities </h1>
<% @cities.each do |city| %>
    <h3> <%= city.name %> </h3>
    <p> Population of <%= city.population %> </p>
    <p> Capitol? <%= city.capitol %> </p>
    <p> State ID: <%= city.state_id %> </p>
<% end %>
